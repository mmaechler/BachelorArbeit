> mkLDL <- function(n, rF = function(n) sample.int(n), rFD = function(n) 1+ abs(rF(n))) {
+     L <- diag(nrow=n)
+     L[lower.tri(L)] <- rF(n*(n-1)/2)
+     list(L = L, D = rFD(n))
+ }
