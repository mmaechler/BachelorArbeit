> 
> ## The function is currently defined as
> function (obj) 
+ {
+     stopifnot(inherits(obj, "fittednorMmix"))
+     k <- obj$k
+     p <- obj$p
+     models <- obj$models
+     val <- matrix(0, length(k), length(models))
+     rownames(val) <- k
+     colnames(val) <- models
+     for (i in seq_along(k)) {
+         for (j in seq_along(models)) {
+             val[i, j] <- npar(k[i], p, models[j])
+         }
+     }
+     val
+   }
function (obj) 
{
    stopifnot(inherits(obj, "fittednorMmix"))
    k <- obj$k
    p <- obj$p
    models <- obj$models
    val <- matrix(0, length(k), length(models))
    rownames(val) <- k
    colnames(val) <- models
    for (i in seq_along(k)) {
        for (j in seq_along(models)) {
            val[i, j] <- npar(k[i], p, models[j])
        }
    }
    val
}
